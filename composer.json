{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.2",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.10.*@stable",
        "undefinedoffset/sortablegridfield": "*",
        "colymba/gridfield-bulk-editing-tools": "*",
        "jonom/focuspoint": "*",
        "symbiote/silverstripe-gridfieldextensions": "*",
        "jonom/silverstripe-betternavigator": "*",
        "symbiote/silverstripe-addressable": "~4.0",
        "md/saml": "dev-md",
        "md/division-analytics": "1.3.*",
        "md/uiowa-bar": "2.*",
        "silverstripe/userforms": "^5.6",
        "silverstripe/tagfield": "^2.8"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/division-bar"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/division-analytics"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-saml"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/uiowa-bar"
        }
    ],
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            ".htaccess",
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/mysite.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php",
            "web.config"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ],
        "expose": [
            "themes/weddings-foundation/dist",
            "themes/weddings-foundation/css"
        ]
    }
}
